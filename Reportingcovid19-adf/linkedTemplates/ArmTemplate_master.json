{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Reportingcovid19-adf"
		},
		"ls_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa'"
		},
		"ls_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdl'"
		},
		"lsHttp_cases_deaths_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/"
		},
		"ls_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://coviddataforreportingdl.dfs.core.windows.net/"
		},
		"cause_death_trigger_properties_pl_ingest_ECDC_parameters_HttpFilePath": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/cases_deaths.csv"
		},
		"cause_death_trigger_properties_pl_ingest_ECDC_parameters_Dest_ECDC_Path": {
			"type": "string",
			"defaultValue": "cause_death.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Reportingcovid19-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa_connectionString')]"
					},
					"ls_adls_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdl_accountKey')]"
					},
					"lsHttp_cases_deaths_properties_typeProperties_url": {
						"value": "[parameters('lsHttp_cases_deaths_properties_typeProperties_url')]"
					},
					"ls_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"cause_death_trigger_properties_pl_ingest_ECDC_parameters_HttpFilePath": {
						"value": "[parameters('cause_death_trigger_properties_pl_ingest_ECDC_parameters_HttpFilePath')]"
					},
					"cause_death_trigger_properties_pl_ingest_ECDC_parameters_Dest_ECDC_Path": {
						"value": "[parameters('cause_death_trigger_properties_pl_ingest_ECDC_parameters_Dest_ECDC_Path')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}